<%@ page language="java" contentType="text/html; charset=US-ASCII"
    pageEncoding="US-ASCII"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
 <head>
 <meta http-equiv="Content-Type" content="text/html; charset=US-ASCII">
  <title>Main Menu</title>
  <script type="text/javascript">
  	function success(){
  		alert("Login Successful!");
  	}
  </script>
 </head>
 <body onload="success()">
  <h1 align="center">What would you like to do?</h1>
  <div id="TransactionType" style="width: 160px; margin-left:auto; margin-right:auto">
   <form method="POST" action="AccountBalance.do">
    <input type="submit" value="View Account Balance" style="width:160px;">
   </form>
   <form method="POST" action="Withdraw.do">
    <input type="submit" value="Withdraw Cash" style="width:160px;">
   </form>
   <form method="POST" action="Deposit.do">
    <input type="submit" value="Make a Deposit" style="width:160px;">
   </form>
  </div>
 </body>
</html>